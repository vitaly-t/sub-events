{
  "name": "sub-events",
  "version": "0.0.1",
  "description": "Events subscription in TypeScript.",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "homepage": "https://github.com/vitaly-t/sub-events",
  "scripts": {
    "all": "npm run build && npm test && npm run lint",
    "build": "tsc && npm run browserify",
    "browserify": "browserify dist/src/index.js -s sub-events -o dist/index.js",
    "doc": "./node_modules/.bin/typedoc",
    "lint": "./node_modules/.bin/tslint --fix ./src/**/*.ts ./test/**/*.ts",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "travis": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc npm test"
  },
  "files": [
    "dist/src",
    "dist/index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitaly-t/sub-events.git"
  },
  "author": {
    "name": "Vitaly Tomilov",
    "email": "vitaly.tomilov@gmail.com"
  },
  "license": "ISC",
  "engines": {
    "node": ">=4.5",
    "npm": ">=2.15"
  },
  "bugs": {
    "url": "https://github.com/vitaly-t/sub-events/issues"
  },
  "keywords": [
    "observable",
    "subscription",
    "counter"
  ],
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/chai-spies": "1.0.0",
    "@types/mocha": "5.2.7",
    "@types/node": "12.6.8",
    "browserify": "16.3.0",
    "chai": "4.2.0",
    "chai-spies": "1.0.0",
    "mocha": "6.2.0",
    "nyc": "^14.1.1",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "typedoc": "0.15.0",
    "typescript": "3.5.3"
  }
}
